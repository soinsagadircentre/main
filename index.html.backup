    <script>
    let translations;
        fetch('translations.json')
           .then(response => response.json())
            .then(data => {
                translations = data;
               // Apply the default language after loading
              const currentLang = localStorage.getItem("lang") || "en";
                applyLanguage(currentLang);
            })
            .catch(error => console.error('Error loading translations:', error));

        document.addEventListener("DOMContentLoaded", () => {
            const langToggle = document.getElementById("langToggle");
            const langMenu = document.getElementById("langMenu");
          const themeToggle = document.getElementById("themeToggle");
          
          // Re-apply language on DOMContentLoaded to ensure all elements are translated
          const currentLang = localStorage.getItem("lang") || "en";
          if (translations) {
              applyLanguage(currentLang);
          }

          langToggle.addEventListener("click", () => {
            langMenu.classList.toggle("show");
        });

        document.querySelectorAll(".tm-lang-option").forEach(opt => {
            opt.addEventListener("click", () => {
                const lang = opt.dataset.lang;
                localStorage.setItem("lang", lang);
                // Apply language with a small delay to ensure all elements are ready
                setTimeout(() => applyLanguage(lang), 10);
                langMenu.classList.remove("show");
            });
        });

        document.addEventListener("click", (e) => {
            if (!langToggle.contains(e.target) && !langMenu.contains(e.target)) {
                langMenu.classList.remove("show");
            }
        });

        const savedTheme = localStorage.getItem("theme") || "light";
        document.body.setAttribute("data-theme", savedTheme);
        updateThemeIcon(savedTheme);

        themeToggle.addEventListener("click", () => {
            const newTheme = document.body.getAttribute("data-theme") === "light" ? "dark" : "light";
            document.body.setAttribute("data-theme", newTheme);
            localStorage.setItem("theme", newTheme);
            updateThemeIcon(newTheme);
        });

        function updateThemeIcon(theme) {
            themeToggle.innerHTML = theme === "dark" ? '<i class="fas fa-sun"></i>' : '<i class="fas fa-moon"></i>';
        }

        function applyLanguage(lang) {
            if (!translations) return; // Wait for JSON to load if not ready

            // Update all elements with data-key attribute
            document.querySelectorAll("[data-key]").forEach(el => {
                const key = el.dataset.key;
                if (translations[lang] && translations[lang][key]) {
                    // For <i> tags inside <h2> or other elements, just update the text content
                    el.textContent = translations[lang][key];
                }
            });

            document.documentElement.lang = lang;
            // Always keep LTR direction to avoid layout changes
            document.body.style.direction = "ltr";
            
            // Force a re-render to ensure changes are visible
            document.body.offsetHeight;
        }
    });

    // Legal pages functionality
    function showLegalPage(pageId) {
        const modal = document.createElement('div');
        modal.id = pageId;
        modal.style.cssText = 'position: fixed; top: 0; left: 0; width: 100%; height: 100%; background: rgba(0,0,0,0.8); z-index: 10000; overflow-y: auto; padding: 20px 0;';
        
        const currentLang = localStorage.getItem("lang") || "en";
        let title = '';
        let lastUpdated = '';
        
        // Get translated title and metadata if translations are available
        if (translations) {
            if (pageId === 'privacy') {
                title = translations[currentLang]['privacy_policy'] || 'Privacy Policy';
            } else if (pageId === 'terms') {
                title = translations[currentLang]['terms_service'] || 'Terms of Service';
            } else if (pageId === 'data-protection') {
                title = translations[currentLang]['data_protection'] || 'Data Protection';
            }
            lastUpdated = translations[currentLang]['policy_last_updated'] || 'Last Updated: November 2025';
        } else {
            // Fallback if translations not loaded
            if (pageId === 'privacy') title = 'Privacy Policy';
            else if (pageId === 'terms') title = 'Terms of Service';
            else if (pageId === 'data-protection') title = 'Data Protection';
            lastUpdated = 'Last Updated: November 2025';
        }
        
        // Build content based on language
        let content = getLocalizedPolicy(pageId, currentLang, title, lastUpdated);
        
        modal.innerHTML = content;
        document.body.appendChild(modal);
        
        // Close modal when clicking outside of it
        modal.addEventListener('click', function(e) {
            if (e.target === this) {
                modal.remove();
            }
        });
    }
    
    // Get localized policy content
    function getLocalizedPolicy(pageId, lang, title, lastUpdated) {
        const policies = {
            en: {
                privacy: `
                    <div style="background: white; max-width: 900px; margin: 20px auto; padding: 40px; border-radius: 8px; box-shadow: 0 4px 20px rgba(0,0,0,0.3); position: relative;">
                        <button onclick="document.getElementById('privacy').remove()" style="position: absolute; top: 15px; right: 15px; background: none; border: none; font-size: 28px; cursor: pointer; color: #666;">&times;</button>
                        <h1>${title}</h1>
                        <p style="color: #666; font-style: italic; margin-bottom: 30px;">${lastUpdated}</p>
                        <h2>1. Introduction</h2>
                        <p>Soins Agadir Center is committed to protecting your privacy...</p>
                        <p style="text-align: center; padding: 20px; color: #999;">English Privacy Policy Content</p>
                        <button onclick="document.getElementById('privacy').remove()" style="background: #0a7fb8; color: white; border: none; padding: 12px 30px; cursor: pointer; margin-top: 30px; border-radius: 5px; font-size: 16px;">Close</button>
                    </div>
                `,
                terms: `
                    <div style="background: white; max-width: 900px; margin: 20px auto; padding: 40px; border-radius: 8px; box-shadow: 0 4px 20px rgba(0,0,0,0.3); position: relative;">
                        <button onclick="document.getElementById('terms').remove()" style="position: absolute; top: 15px; right: 15px; background: none; border: none; font-size: 28px; cursor: pointer; color: #666;">&times;</button>
                        <h1>${title}</h1>
                        <p style="color: #666; font-style: italic; margin-bottom: 30px;">${lastUpdated}</p>
                        <h2>1. Agreement to Terms</h2>
                        <p>By accessing and using Soins Agadir Center services...</p>
                        <p style="text-align: center; padding: 20px; color: #999;">English Terms of Service Content</p>
                        <button onclick="document.getElementById('terms').remove()" style="background: #0a7fb8; color: white; border: none; padding: 12px 30px; cursor: pointer; margin-top: 30px; border-radius: 5px; font-size: 16px;">Close</button>
                    </div>
                `,
                'data-protection': `
                    <div style="background: white; max-width: 900px; margin: 20px auto; padding: 40px; border-radius: 8px; box-shadow: 0 4px 20px rgba(0,0,0,0.3); position: relative;">
                        <button onclick="document.getElementById('data-protection').remove()" style="position: absolute; top: 15px; right: 15px; background: none; border: none; font-size: 28px; cursor: pointer; color: #666;">&times;</button>
                        <h1>${title}</h1>
                        <p style="color: #666; font-style: italic; margin-bottom: 30px;">${lastUpdated}</p>
                        <h2>1. Commitment to Confidentiality</h2>
